{
    "title": "koca dashboard",
    "description": "koca dashboard",
    "language": "zh-hans",
    "author": "szkingdom.com",
    "links": {
        "sidebar": {
            "金证科技": "https://www.szkingdom.com"
        }
    },
    "plugins": [
        "codesnippet",
        "splitter",
        "page-toc-button",
        "image-captions",
        "back-to-top-button",
        "-lunr", "-search", "search-plus",
        "favicon@^0.0.2",
        "theme-default",
        "tbfed-pagefooter@^0.0.1",
        "-highlight", "prism", "prism-themes",
        "lightbox",
        "mermaid-v8"
    ],
    "pluginsConfig": {
        "theme-default": {
            "showLevel": true
        },
        "image-captions": {
            "caption": "图 _PAGE_LEVEL_._PAGE_IMAGE_NUMBER_：_CAPTION_"
        },
        "tbfed-pagefooter": {
            "copyright": "Copyright ©2019-2020 wuzb",
            "modify_label": " Updated at ",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "favicon": {
            "shortcut": "favicon.ico",
            "bookmark": "favicon.ico"
        },
        "prism": {
            "css": [
            "prism-themes/themes/prism-ghcolors.css"
            ]
        }
    }
}
